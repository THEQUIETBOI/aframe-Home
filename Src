<script>
 
 let isOn = true;  // Initial state (assuming it starts visible)


document.querySelector('#CASEOH').addEventListener('mouseenter', function () {
 
var math = Math.floor(Math.random()* 4) + 1;
if (math == 1 && isOn){

  this.setAttribute('src', "https://qph.cf2.quoracdn.net/main-qimg-57337d6c38d8603192016e45807d3828-lq");
}
if (math == 2 && isOn){
  this.setAttribute('src', "https://assets.change.org/photos/6/oz/rk/eKozRKFDQpdaZxV-800x450-noPad.jpg?1628010770");
}
if (math == 3 && isOn){
  this.setAttribute('src', "https://upload.wikimedia.org/wikipedia/commons/thumb/7/7c/Cima_da_Conegliano%2C_God_the_Father.jpg/300px-Cima_da_Conegliano%2C_God_the_Father.jpg");
}
if (math == 4 && isOn){
  this.setAttribute('src', "https://i1.sndcdn.com/artworks-gtUOb4ZmaytZEx1B-nHj1Ww-t500x500.jpg");
}


  if (isOn) {  // Check if it's currently visible
  
    this.setAttribute('visible', false);
  
    isOn = false;  // Update the flag

    
  } else {  // Otherwise, it's currently hidden
    this.setAttribute('visible', true);  // Show it
    isOn = true;  // Update the flag
    
  }
});

</script>

<script>
    const box = document.querySelector('#my-box');
const audio = new Audio('https://cdn.jsdelivr.net/gh/THEQUIETBOI/aframe-Home@main/AUDIO/sticking-out-ur-gyat-for-the-rizzler.mp3');

box.addEventListener('mouseleave', () => {
  audio.play();
});

</script>

